    "@commitlint/config-conventional": "^17.6.3",
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.0",
    "pinst": "^3.0.0",
    "prettier": "^2.5.1",
    "turbo": "^1.9.7"
  },
  "name": "damons-ui",
  "packageManager": "yarn@3.5.1",
  "workspaces": [
    "apps/*",
    "packages/*"
